{
  "version": 2,
  "name": "iqra-expense-tracker-2024",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "github": {
    "silent": true
  },
  "env": {
    "VITE_API_URL": "@vite_api_url"
  }
}
